<odoo>
  <record id="view_hr_hospital_doctor_speciality_list" model="ir.ui.view">
    <field name="name">hr.hospital.doctor.speciality.list</field>
    <field name="model">hr.hospital.doctor.speciality</field>
    <field name="arch" type="xml">
      <list>
        <field name="name"/>
        <field name="code"/>
        <field name="description"/>
        <field name="active"/>
      </list>
    </field>
  </record>

  <record id="view_hr_hospital_doctor_speciality_form" model="ir.ui.view">
    <field name="name">hr.hospital.doctor.speciality.form</field>
    <field name="model">hr.hospital.doctor.speciality</field>
    <field name="arch" type="xml">
      <form>
        <sheet>
          <group>
        <field name="name"/>
        <field name="code"/>
        <field name="description"/>
        <field name="active"/>
          </group>

        </sheet>
      </form>
    </field>
  </record>

  <record id="action_hr_hospital_doctor_speciality" model="ir.actions.act_window">
    <field name="name">DoctorSpeciality</field>
    <field name="type">ir.actions.act_window</field>
    <field name="res_model">hr.hospital.doctor.speciality</field>
    <field name="view_mode">list,form</field>
  </record>
</odoo>
